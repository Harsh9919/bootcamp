#include <stdio.h>

int main() {
    // Write C code here
   int a,b,c,x,y,z;
   scanf("%d",&a);
   scanf("%d",&b);
   scanf("%d",&c);
   scanf("%d",&x);
   scanf("%d",&y);
   scanf("%d",&z);
   
   if(a+b+c==180){
    if(x+y>z||y+z>x||x+z>y){
       printf("it is valid traingle");
   }
   else {
       printf("it is not valid");
       
   }
   }
   else{
       printf("it is not valid");
   }
   

    return 0;
}
